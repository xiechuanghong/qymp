/* pages/likemember/likemember.wxss */
.all {
  height: 64rpx;
  line-height: 64rpx;
  background-color: #f2f2f2;
  padding-left: 32rpx;
  color: #8b9199;
}

.item {
  display: flex;
  align-items: center;
  background-color: #fff;
  padding: 16rpx 32rpx;
  border-bottom: 1px solid rgba(0, 0, 0, .1)
}

.item image {
  width: 64rpx;
  height: 64rpx;
  margin-right: 16rpx;
  border-radius: 8rpx;
}

.item .text {
  flex: 1;
  color: #576B94;
}